{
  "name": "glob-interceptor",
  "version": "0.0.1",
  "description": "Bring your own file system for globbing by proxying the cache",
  "main": "dist/index",
  "files": [
    "dist/*.*"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "test": "ava dist/test",
    "lint": "run-p lint:*",
    "lint:valtyr": "wotan -m @fimbul/valtyr",
    "lint:wotan": "wotan",
    "verify": "run-s compile lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajafff/glob-interceptor.git"
  },
  "keywords": [
    "glob",
    "fs",
    "memory",
    "virtual",
    "cache",
    "proxy",
    "intercept",
    "interceptor"
  ],
  "author": "Klaus Meinhardt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajafff/glob-interceptor/issues"
  },
  "homepage": "https://github.com/ajafff/glob-interceptor#readme",
  "devDependencies": {
    "@fimbul/valtyr": "^0.14.0",
    "@fimbul/wotan": "^0.14.0",
    "@types/glob": "^5.0.35",
    "@types/node": "^10.9.4",
    "ava": "^0.25.0",
    "glob": "7.1.3",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.11.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "typescript": "~3.0.3"
  },
  "dependencies": {
    "memfs": "^2.10.0"
  }
}
